{
  "name": "@carno.js/swagger",
  "version": "0.2.6",
  "description": "Generate automatically swagger.json from your carno.js project",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "rm -rf ./dist tsconfig.tsbuildinfo && tsc --build --force",
    "build": "tsc --build --force",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "openapi-types": "^12.1.3",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@types/swagger-ui": "^3.52.2"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:carnojs/carno.js.git"
  },
  "peerDependencies": {
    "@carno.js/core": "file://../core"
  },
  "author": "Lucas Mota",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "carno",
    "carno.js",
    "swagger"
  ],
  "gitHead": "726879b8d699b9abf35e80aa64ada630c8ced14d"
}
