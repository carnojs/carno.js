{
  "name": "@carno.js/orm",
  "version": "1.1.1",
  "description": "A simple ORM for Carno.js.",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "require": "./dist/testing/index.js",
      "default": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "compile": "rm -rf ./dist tsconfig.tsbuildinfo && tsc --build --force",
    "build": "tsc --build --force",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "globby": "^10.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-morph": "^27.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:carnojs/carno.js.git"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.6",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@carno.js/core": "^1.0.0"
  },
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "carno",
    "carno.js",
    "orm",
    "database",
    "sql",
    "postgres",
    "postgresql",
    "bun",
    "value-object"
  ],
  "gitHead": "d2b000eeb352a03569b90d8eb2bf3557c00b741b"
}
