{
  "name": "@carno.js/core",
  "version": "0.2.9",
  "description": "Carno.js is a framework for building web applications object oriented with TypeScript and Bun.sh",
  "keywords": [
    "bun",
    "framework",
    "typescript",
    "carno",
    "carno.js",
    "object oriented",
    "oop"
  ],
  "type": "commonjs",
  "scripts": {
    "compile": "rm -rf ./dist tsconfig.tsbuildinfo && tsc --build --force",
    "build": "tsc --build --force",
    "prepublishOnly": "bun run build"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:carnojs/carno.js.git"
  },
  "license": "MIT",
  "dependencies": {
    "bentocache": "^1.0.0-beta.5",
    "globby": "^10.0.2",
    "parseurl-fast": "^1.2.0",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/globby": "^9.1.0",
    "bun-types": "latest",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3"
  },
  "peerDependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "class-validator": {
      "optional": true
    },
    "class-transformer": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "726879b8d699b9abf35e80aa64ada630c8ced14d"
}
